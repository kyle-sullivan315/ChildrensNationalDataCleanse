Sub FindDisease()
    Set ws1 = Worksheets("All Data")
    Set ws2 = Worksheets("All Disorders")
    lastRow = Cells(Rows.Count, "A").End(xlUp).Row
    
    RowCount = 1
    rowCountTwo = 787
    With Worksheets("All Data").Range("AI1")
        Do Until .Offset(RowCount, 0).Value = ""
            If UCase(.Offset(RowCount, 0).Value) = 1 Then
                ws1.Rows(RowCount + 1).EntireRow.Copy ws2.Range("A" & rowCountTwo + 1)
                rowCountTwo = rowCountTwo + 1
            End If
            RowCount = RowCount + 1
        Loop
    End With
End Sub

Sub removeDups()
Worksheets("All Disorders").Range("A1:BNE1003").RemoveDuplicates Columns:=1, Header:=xlYes

End Sub


Sub Condense_neurosurg()

    RowCount = 1
    With Worksheets("All Disorders Test 2").Range("K1")
        Do Until .Offset(RowCount, 0).Value = ""
            If UCase(.Offset(RowCount, 0).Value) = 1 Then
                target = "K" + Str(RowCount)
                .Offset(RowCount, 0).Value = 0
            End If
            RowCount = RowCount + 1
        Loop
    End With
    
    RowCount = 1
    With Worksheets("All Disorders Test 2").Range("L1")
        Do Until .Offset(RowCount, 0).Value = ""
            If UCase(.Offset(RowCount, 0).Value) = 1 Then
                .Offset(RowCount, -1).Value = 1
            End If
            RowCount = RowCount + 1
        Loop
    End With
    
    RowCount = 1
    With Worksheets("All Disorders Test 2").Range("M1")
        Do Until .Offset(RowCount, 0).Value = ""
            If UCase(.Offset(RowCount, 0).Value) = 1 Then
                .Offset(RowCount, -2).Value = 1
            End If
            RowCount = RowCount + 1
        Loop
    End With
    
    RowCount = 1
    With Worksheets("All Disorders Test 2").Range("N1")
        Do Until .Offset(RowCount, 0).Value = ""
            If UCase(.Offset(RowCount, 0).Value) = 1 Then
                .Offset(RowCount, -3).Value = 1
            End If
            RowCount = RowCount + 1
        Loop
    End With
    
    RowCount = 1
    With Worksheets("All Disorders Test 2").Range("O1")
        Do Until .Offset(RowCount, 0).Value = ""
            If UCase(.Offset(RowCount, 0).Value) = 1 Then
                .Offset(RowCount, -4).Value = 1
            End If
            RowCount = RowCount + 1
        Loop
    End With
    
    Worksheets("All Disorders Test 2").Range("L1:P630").Delete Shift:=xlToLeft
    
End Sub
